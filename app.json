{
  "pages": [
    "pages/discover/index",
    "pages/shoot/index",
    "pages/market/index",
    "pages/message/index",
    "pages/profile/index",
    "pages/release/index",
    "pages/detail/index"
  ],
  "usingComponents": {
    "t-toast": "tdesign-miniprogram/toast/toast"
  },
  "subpackages": [
    {
      "root": "pages/works-detail",
      "name": "works-detail",
