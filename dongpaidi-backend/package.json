{
  "name": "dongpaidi-backend",
  "version": "1.0.0",
  "description": "懂拍帝摄影社交平台后端API服务",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "test-server": "nodemon src/test-server.ts",
    "test-api": "nodemon src/test-api.ts",
    "test-postgresql": "ts-node src/test-postgresql.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "db:setup": "chmod +x scripts/setup-database.sh && ./scripts/setup-database.sh",
    "db:migrate-from-sqlite": "ts-node scripts/migrate-to-postgresql.ts",
    "docker:dev": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh",
    "docker:start": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh start",
    "docker:stop": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh stop",
    "docker:clean": "chmod +x scripts/docker-dev.sh && ./scripts/docker-dev.sh clean",
    "admin:install": "cd admin-panel && npm install",
    "admin:dev": "cd admin-panel && npm run dev",
    "admin:build": "cd admin-panel && npm run build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "secure-start": "ts-node src/secure-server.ts",
    "secure-dev": "nodemon src/secure-server.ts",
    "generate-keys": "ts-node src/secure-server.ts --generate-keys",
    "security-test": "ts-node src/test-security.ts",
    "security-check": "ts-node src/secure-server.ts --security-test",
    "test:security": "npm run security-test",
    "db:optimize": "ts-node src/test-db-optimization.ts",
    "log-analysis-test": "ts-node -r module-alias/register src/test-log-analysis.ts",
    "test:logs": "npm run log-analysis-test",
    "simple-log-test": "ts-node -r module-alias/register src/simple-log-test.ts",
    "performance-test": "ts-node -r module-alias/register src/basic-performance-test.ts",
    "test:performance": "npm run performance-test",
    "perf-test": "npm run performance-test",
    "performance-full-test": "ts-node -r module-alias/register src/test-performance-monitoring.ts"
  },
  "keywords": [
    "photography",
    "social",
    "api",
    "express",
    "typescript",
    "prisma"
  ],
  "author": "DongPaiDi Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@types/redis": "^4.0.10",
    "alipay-sdk": "^4.14.0",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.7.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "^5.6.0",
    "redis": "^4.7.1",
    "sharp": "^0.32.6",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "wechatpay-node-v3": "^2.2.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.8.10",
    "@types/socket.io": "^3.0.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "_moduleAliases": {
    "@": "src"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
